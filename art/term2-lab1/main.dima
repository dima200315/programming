int obrabotka(int n, int m, int a[10][10], int ii, int jj) {
    int first = a[ii][jj];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            bool diagonal = ((i == (j + ii - jj)));
            bool horisont = (i==ii && j<=jj);
            if (diagonal || horisont) {
                if (a[i][j] > 0) {
                    int e = a[i][j];
                    first = nok(first, e);
                }
            }
        }
    }
    if (first < 0) {
        first = 0;
    }
    return first;
}
int nok(int x, int y) {
    if (x <= 0) {
        return y;
    }
    int count = (x > y) ? x : y;
    for (int i = count;; i++) {
        if (!(i % x) && !(i % y)) {
            return i;
        }
    }
}
